<script type="text/javascript">
    /**
     * 获取fixHeightUrl
     * @returns {string}
     */
    var getTyHostUrl = function() {
        var _url = "";
        $.ajax({
            url: "/jf189/ty/hostUrl",
            type: 'get',
            async: false,
            dataType:'json',
            success: function (data) {
                _url = data.url;
            }
        });
        return _url;
    }
    var getIP = function(){
        var province = '';
        var ip = getIPv4(function(ip){
            $.ajax({
                url: "/nnc/province?clientIp=" + ip,
                type: 'get',
                dataType: 'json',
                success: function (rdata) {
                    if(rdata.status === 200) {
                        province = rdata.data.provinceId;
                    }
//                    window.location.href = _randerUrl + "&provinceId=" + province + "&storehouseId=-1";
                }
            });
        });
        return province;
    }
    $(function(){
//            contentLoaded(resizeIfr);
        resizeIfr();
        var province = getIP() || 110000;
        $("a.detail").on("click",function(){
            var href=$(this).attr("data_href")+ "&provinceId=" + province + "&storehouseId=-1";
            $(this).attr("href",href);
        });
    });
    function resizeIfr() {
        var _tyHostUrl = getTyHostUrl();
        var doc = document;
//        var height = calcPageHeight(doc);
        var myifr = doc.getElementById('iframeA');
        myifr.src = _tyHostUrl + '/CommPage/fixHeight.html#5409';
    }

    // 计算页面的实际高度，iframe自适应会用到
    function calcPageHeight(doc) {
        var h = $(document.body).height();

        var cHeight = Math.max(doc.body.clientHeight, doc.documentElement.clientHeight)
        var sHeight = Math.max(doc.body.scrollHeight, doc.documentElement.scrollHeight)
        var height  = Math.max(cHeight, sHeight)
        console.log("cH:"+cHeight+"| sH:"+sHeight+"| h:" +h);
        return height;
    }
</script>
